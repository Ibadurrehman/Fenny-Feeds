@model IEnumerable<FennyFeeds.ViewModels.VM_Product>
@{
    ViewBag.Title = "WishList";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}
<style type="text/css">
    .shopping-summary {
        background-color: #fff;
        width: 100%;
        height: auto;
        display: inline-block;
        padding-top: 30px;
        padding-bottom: 50px;
    }

    .order-strip {
        width: 100%;
        height: 70px;
        position: relative;
        border-bottom: 1px solid #b1b1b1;
    }

    .container {
        width: 1280px;
    }

    .order-strip h1 {
        font-weight: bold;
        color: #333;
        position: absolute;
        margin: 0;
        top: 20px;
    }

    .order-strip span {
        font-size: 14px;
        font-weight: bold;
        float: right;
        color: #333;
        margin-top: 40px;
    }

    .order-summary {
        width: 100%;
        height: auto;
        display: inline-block;
        margin-top: 50px;
        margin-bottom: 50px;
    }

    .order-steps {
        width: 100%;
        height: auto;
        display: inline-block;
        padding-left: 0;
    }

    ul.order-steps li {
        float: left;
        width: 20%;
        text-align: left;
        border: 1px solid;
        border-top-color: #cacaca;
        border-bottom-color: #b1b1b1;
        border-right-color: #b7b7b7;
        border-left-color: #b7b7b7;
        background-color: #f5f5f5;
    }

    li.step_current {
        background-color: #b3b3b3 !important;
    }

    .order-steps li {
        list-style: none;
        float: left;
    }

    ul.order-steps li span {
        display: block;
        border: 1px solid;
        border-color: #fff;
        color: #333;
        position: relative;
        line-height: 49px;
        padding-left: 40px;
        font-weight: bold;
        font-size: 16px;
    }

    li.step_current span {
        color: #fff !important;
        border-color: #ccc !important;
    }

    ul.order-steps li span:after {
        content: ".";
        position: absolute;
        top: 0;
        right: -31px;
        z-index: 5;
        text-indent: -5000px;
        display: block;
        width: 31px;
        height: 52px;
        margin-top: -2px;
    }

    .pr-order-details {
        width: 100%;
        height: auto;
        display: inline-block;
    }

    .table-bordered {
        border: 1px solid #ddd;
    }

    .order-table > thead:first-child > tr:first-child > th {
        text-align: center;
    }

    .responsive {
        width: 100%;
        height: auto;
    }

    .order-btn {
        background-color: #333;
        border-radius: 0px;
        color: #fff;
        border: 0;
    }

    .width15 {
        width: 15%;
        text-align: center;
        font-weight: bold;
    }

    .width30 {
        width: 30%;
    }

    .width5 {
        width: 5%;
    }

    .width10 {
        width: 10%;
    }
</style>
<div class="shopping-summary">
    <div class="order-strip">
        <div class="container">
            <h1>Your WishList Summary</h1>
            <span>Your wishlist cart contains: 1 product</span>
        </div>
    </div>
    <div class="order-summary">
        @*<div class="container">
            <ul class="order-steps">
                <li class="step_current"><span>01. Summary</span></li>
            </ul>
        </div>*@
    </div>
    <div class="pr-order-details">
        <div class="container" id="_wishlist">
           @* <table class="table table-bordered order-table" id="_wishlistTable">
                <thead style="background-color: #f5f5f5">
                    <tr>
                        <th class="width15">Product
                        </th>
                        <th class="width30">Product Name
                        </th>
                        <th class="width10">Product Code
                        </th>
                        <th class="width15">Unit price
                        </th>
                        <th class="width10">Discount %
                        </th>
                        <th class="width15">Total
                        </th>
                        <th class="width5"></th>
                    </tr>
                </thead>
                <tbody>
                   
                </tbody>
            </table>*@
        </div>
        <div class="container">
            <div class="btn-group">
                <button type="button" class="btn btn-warning order-btn" style="margin-right: 10px">
                    <i class="fa fa-chevron-left" style="margin-right: 10px"></i>Continue Shopping</button>
                <button type="button" class="btn btn-warning order-btn">
                    Proceed to Checkout<i class="fa fa-chevron-right" style="margin-left: 10px"></i></button>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        Get_WishList();
    });
</script>
